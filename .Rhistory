##TIPO PROMEDIO
ss_uy<-function(alpha=0.05,cv=1,er,ff=FALSE,N,efect=1,tnr=0){
z<-qnorm(1-alpha/2)
n0<-((z^2)*(cv^2))/(er^2)
n<-n0
if(isTRUE(ff)){
n<-n0/(1+(n0/N))
}
nf<-(n*efect)/(1-tnr)
return(nf)
}
